#include <stdio.h>
#include <conio.h>

int main()
{
    int a,b,c;
    printf("enter a three number :\n");
    scanf("%d%d%d",&a,&b,&c);
    printf("\n");
    if(a>b)
        if(a>c)
        printf("%d is the greatest",a);
        else
        printf("%d",c);

    else if(b>c)
        printf("%d is the greatest",b);
        else
        printf("%d is the greatest",c);
    return 0;

}
